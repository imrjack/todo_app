<!DOCTYPE html>
<html>
<head>
  <title>TodoApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>

</head>
<body>
  <header id='top' tabindex='-1'>
  <nav class='navbar-default' id= 'content'>
    <div class='container-fluid'>
        <%= render 'layouts/navigation' %>
    </div>
  </nav>
  </header>
  <%= render 'layouts/msg' %>
  <%=yield%>
</body>
</html>
